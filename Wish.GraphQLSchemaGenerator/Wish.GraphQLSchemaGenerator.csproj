<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
	  <Authors>Wish Org</Authors>
	  <Company>Wish Org</Company>
	  <Product></Product>
	  <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	  <PackageProjectUrl>https://github.com/Wish-Org/Wish.GraphQLSchemaGenerator</PackageProjectUrl>
	  <RepositoryUrl>https://github.com/Wish-Org/Wish.GraphQLSchemaGenerator</RepositoryUrl>
	  <Description>Auto-generate c# types by using GraphQL schema introspection</Description>
	  <Version>1.0</Version>
	  <PackageTags>graphql types classes schema generator strongly typed</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="4.7.0" />
    <PackageReference Include="System.Interactive" Version="6.0.1" />
  </ItemGroup>

</Project>
